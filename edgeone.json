{
  "headers": [
    {
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ],
      "source": "/api/*"
    },
    {
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ],
      "source": "/zhilu.opml"
    },
    {
      "source": "/_ipx/*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/_nuxt/*",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=36000, immutable" }
      ]
    }
  ],
  "caches": [
    {
      "source": "/_ipx/*",
      "cacheTtl": 31536000
    },
    {
      "source": "/_nuxt/*",
      "cacheTtl": 36000
    }
  ],
  "redirects": [
    {
      "destination": "https://www.zhilu.site/api/icon",
      "source": "/favicon.ico",
      "status": 308
    }
  ]
}