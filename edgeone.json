{
  "rewrites": [
    {
      "source": "/api/bilibili/(.*)",
      "destination": "https://api.bilibili.com/x/space/",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ],
  "headers": [
    {
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ],
      "source": "/api/*"
    },
    {
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ],
      "source": "/zhilu.opml"
    }
  ],
  "redirects": [
    {
      "destination": "https://www.zhilu.site/api/icon",
      "source": "/favicon.ico",
      "status": 308
    }
  ]
}